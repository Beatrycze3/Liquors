@model  Liquors.ViewModels.RegisterModel


<div class="container">
    <div class="row">
        <div class="col-8">
            <div class="page-header">
                <h1>Register form</h1>
            </div>
            @using (Html.BeginForm("Register", "Account", FormMethod.Post))
            {
                @Html.EditorForModel();
                @Html.ValidationSummary();
                <button>Register</button>
            }
       
        </div>
        <div class="col-4">
            <a class="btn btn-link" href="@Url.Action("LogIn")">Sign In</a>
        </div>

    </div>
    
    
</div>